@article{Pap2009_ICOSSAR,
	Abstract = {In order to motivate the incorporation of the reliability concept in the structural design and analysis procedure, the engineering community is in need of finite-element software with capabilities of in- cluding the stochastic nature of input parameters. This paper presents a reliability tool to be integrated into the SOFiSTiK finite element software package. SOFiSTiK is a powerful program that encompasses a variety of finite elements as well as meshing and CAD modules. The reliability tool is programmed in a separate module while the interaction is established by communication with the core's database. The tool includes FORM ap- proximation with robust optimization algorithms and a variety of simulation techniques, including importance and directional Monte Carlo integration of the failure domain. The representation of the response surface with a polynomial meta-model is also included. The paper discusses the reliability methods and gives examples to verify their applicability to industrial problems.},
	Author = {I. Papaioannou and A. D{\"u}ster and E. Rank and H. Heidkamp and C. Katz},
	Date-Added = {2010-03-10 18:15:11 +0100},
	Date-Modified = {2010-03-12 14:43:45 +0100},
	Journal = {Proceedings of the 10th International Conference on Structural Safety and Reliability (ICOSSAR2009)},
	Month = {September 13 - 17,},
	Title = {Integration of reliability methods into a commercial finite element software package},
	Volume = {Osaka, Japan},
	Year = {2009}}

@article{Pap2008,
	Abstract = {In this paper, a number of reliability methods are investigated with a focus on the improvement of accuracy and efficiency, aiming at the application to large-scale in- dustrial problems. These methods include approximation and simulation approaches, such as the widely known First-Order Reliability Method (FORM) and the Directional Sampling (DS) concept. The methods are optimized, to increase their robustness and keep computational cost at low levels, in order to extend their applicability to a wide range of problems. A series of examples is provided, computed by coupling the relia- bility methods with a commercial finite element software package.},
	Author = {I. Papaioannou and H. Heidkamp and A. D{\"u}ster and E. Rank and C. Katz},
	Date-Added = {2010-03-10 17:54:44 +0100},
	Date-Modified = {2010-03-12 14:44:09 +0100},
	Journal = {Proceedings of the Ninth International Conference on Computational Structures Technology},
	Keywords = {Stochastic; Reliability; FORM; Directional sampling; Finite element analysis; Industrial applications; Approximation; Simulation},
	Month = {September 2 - 5,},
	Title = {Towards Efficient Reliability Methods with Applications to Industrial Problems},
	Volume = {Athens, Greece},
	Year = {2008}}

@article{Pap2009,
	Abstract = {This paper presents an application of the subset simulation method to the finite element reliability assessment of a deep circular tunnel. The tunnel is assumed to be surrounded by weak rock with uncertain material properties. For the spatial variability of the uncertain parameters the midpoint method is utilized. Depending on the assumed correlation length, the above stochastic discretization method may lead to a very large number of random variables. In such cases, most Monte Carlo variants prove to be inefficient in computing small failure probabilities, which are typical in structural engineering. How- ever, the subset simulation method overcomes this problem by expressing the failure probability as a product of larger conditional probabilities, which are es- timated by applying an efficient Markov chain Monte Carlo technique. The re- liability analysis is performed using a reliability tool, which is being developed as part of the SOFiSTiK finite element software package.},
	Author = {I. Papaioannou and H. Heidkamp and A. D{\"u}ster and S. Kollmannsberger and E. Rank and C. Katz},
	Date-Added = {2010-03-10 16:50:31 +0100},
	Date-Modified = {2010-03-12 14:44:28 +0100},
	Journal = {Proceedings of the 7th International Probabilistic Workshop},
	Month = {November 25 - 26,},
	Read = {No},
	Title = {The subset simulation applied to the reliability analysis of a nonlinear geotechnical finite element method},
	Volume = {Delft, The Netherlands},
	Year = {2009}}

@article{Schueller2007,
	Abstract = {
In this paper the need for a rational treatment of uncertainties in structural mechanics and analysis is reasoned. It is shown that the traditional deterministic conception can be easily extended by applying statistical and probabilistic concepts. The so-called Monte Carlo simulation procedure is the key for those developments, as it allows the straightforward use of the currently used deterministic analysis procedures.
A numerical example exemplifies the methodology. It is concluded that uncertainty analysis may ensure robust predictions of variability, model verification, safety assessment, etc.},
	Author = {G.I. Schu{\"e}ller},
	Date-Modified = {2010-03-10 17:49:56 +0100},
	Doi = {DOI: 10.1016/j.compstruc.2006.10.009},
	Issn = {0045-7949},
	Journal = {Computers \& Structures},
	Keywords = {Uncertainty; Monte Carlo simulaton; Finite elements; Response variability; Model verification; Robustness},
	Number = {5-6},
	Pages = {235 - 243},
	Title = {On the treatment of uncertainties in structural mechanics and analysis},
	Url = {http://www.sciencedirect.com/science/article/B6V28-4MMP28M-1/2/304230b72e1ecef2c2fb0e6170e3c3f5},
	Volume = {85},
